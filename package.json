{
  "name": "@sigodenh/dee",
  "version": "0.2.2",
  "description": "Dee 框架",
  "main": "./src/index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns services --coverage",
    "test:srvs": "jest --testPathPattern services --coverage",
    "pretest:srvs": "docker-compose up -d",
    "posttest:srvs": "docker-compose down"
  },
  "keywords": [
    "dee",
    "ddd",
    "swagger",
    "express"
  ],
  "author": "sigoden huang <sigoden@gmail.com> (https://sigoden.com)",
  "homepage": "https://github.com/sigoden/dee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigoden/dee.git"
  },
  "dependencies": {
    "@sigodenh/dee-swaggerize": "^0.1.2",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "ioredis": "^3.2.2",
    "jest": "^22.4.3",
    "mongoose": "^5.0.17",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.7",
    "supertest": "^3.0.0",
    "winston": "^3.0.0-rc5"
  },
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=4"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/dist/"
    ],
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ]
  }
}
