{
  "name": "@sigoden/swag",
  "version": "0.1.0",
  "description": "Swag 开发框架",
  "main": "./src/index.js",
  "publishConfig": {
    "registry": "https://npm.sigoden.com"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "test": "RUNNING_SWAGGER_TOOLS_TESTS=true jest --coverage --testPathIgnorePatterns services",
    "test:srvs": "jest --coverage --testPathPattern services",
    "pretest:srvs": "docker-compose up -d",
    "posttest:srvs": "docker-compose down"
  },
  "keywords": ["swagger", "express"],
  "author": "sigoden huang <sigoden@gmail.com> (https://sigoden.com)",
  "homepage": "https://github.com/sigoden/swag",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sigoden/swag.git"
  },
  "dependencies": {
    "express": "^4.16.3",
    "swagger-tools": "^0.10.3"
  },
  "devDependencies": {
    "ioredis": "^3.2.2",
    "jest": "^22.4.3",
    "mongoose": "^5.0.17",
    "mysql2": "^1.5.3",
    "sequelize": "^4.37.7",
    "supertest": "^3.0.0",
    "winston": "^3.0.0-rc5"
  },
  "files": [
    "src"
  ],
  "jest": {
    "coverageDirectory": "../coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/",
      "/dist/"
    ],
    "testEnvironment": "node",
    "rootDir": "./src",
    "roots": [
      "../test"
    ]
  }
}
