{
  "name": "@sigodenjs/dee-grpc",
  "version": "0.8.0",
  "description": "A rpc service for dee framework, powered by grpc",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint --project tsconfig.json \"{lib,test}/**/*.ts\"",
    "test:unit": "jest",
    "test": "npm-run-all --parallel lint test:unit",
    "build": "tsc",
    "fmt": "npm-run-all --parallel fmt:ts && npm run -s lint -- --fix",
    "fmt:ts": "prettier --single-quote --trailing-comma all --parser typescript --print-width 100 --write \"{lib,test}/**/*.ts\"",
    "prepublish": "npm run -s build"
  },
  "keywords": [],
  "author": "Sigoden Huang <sigoden@gmail.com> (https://sigoden.com/)",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "@sigodenjs/dee": "^0.8.0",
    "grpc": "^1.14.1",
    "typescript": "^3.0.3"
  }
}
